{
  "name": "logseq-live-math",
  "version": "1.4.3",
  "description": "Type LaTeX in live mode!",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts src/parent.ts --minify --bundle --loader:.css=text --outdir=dist",
    "format": "prettier -w src/ README.md index.html .github/"
  },
  "keywords": [],
  "author": "Allan Chain",
  "license": "MIT",
  "dependencies": {
    "mathlive": "^0.102.0"
  },
  "devDependencies": {
    "@logseq/libs": "^0.0.17",
    "@tsconfig/recommended": "^1.0.8",
    "esbuild": "^0.24.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1"
  },
  "logseq": {
    "title": "Live Math",
    "id": "logseq-live-math",
    "main": "index.html",
    "icon": "icon.svg"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{ts,css,md,html,json}": "prettier --write"
  },
  "packageManager": "pnpm@9.1.1+sha256.9551e803dcb7a1839fdf5416153a844060c7bce013218ce823410532504ac10b"
}
