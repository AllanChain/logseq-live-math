{
  "name": "logseq-live-math",
  "version": "1.2.4",
  "description": "Type LaTeX in live mode!",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts src/parent.ts --minify --bundle --loader:.css=text --outdir=dist",
    "format": "prettier -w src/ README.md index.html .github/"
  },
  "keywords": [],
  "author": "Allan Chain",
  "license": "MIT",
  "dependencies": {
    "mathlive": "^0.98.3"
  },
  "devDependencies": {
    "@logseq/libs": "^0.0.15",
    "@tsconfig/recommended": "^1.0.3",
    "esbuild": "^0.19.8",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0"
  },
  "logseq": {
    "title": "Live Math",
    "id": "logseq-live-math",
    "main": "index.html",
    "icon": "icon.svg"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{ts,css,md,html,json}": "prettier --write"
  }
}
